<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.didi.hummer.demo">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_hummer"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HummerSinglePageActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".HummerSinglePageFragmentActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".HummerEmbeddedPageActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".HummerDelegatePageActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
    </application>

</manifest>