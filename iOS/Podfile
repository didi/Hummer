# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'App' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Example
  pod 'Hummer', :path => '..', :subspecs => ["Hermes", "OSSYoga"]
  pod 'Yoga'

  target 'AppTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Quick', '~> 4.0'
    pod 'Nimble', '~> 9.0'
  end

end
